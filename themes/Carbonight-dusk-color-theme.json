  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#7b809a" }
    },
    {
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": { "foreground": "#d2d6f0" }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#8fd3f4" }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "variable.function"
      ],
      "settings": { "foreground": "#f08fa1" }
    },
    {
      "scope": ["string"],
      "settings": { "foreground": "#c6f6c2" }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": { "foreground": "#f1b08e" }
    },
    {
      "scope": ["entity.name.class", "support.class"],
      "settings": { "foreground": "#d1b3ff" }
    },
    {
      "scope": ["markup.heading"],
      "settings": { "foreground": "#C3E88D", "fontStyle": "bold" }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#F28C8C" }
    }
  ]
}
